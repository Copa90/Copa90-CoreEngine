{
  "name": "exact",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "topic": {
      "type": "string",
      "required": true
    },
    "point": {
      "type": "number",
      "required": true
    },
    "beginningTime": {
      "type": "number",
      "required": true
    },
    "endingTime": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "string",
      "required": false,
      "default": "Created"
    },
    "label": {
      "type": "string",
      "required": true
    },
    "answer": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "selectors": {
      "type": [
        "object"
      ]
    }
  },
  "validations": [],
  "relations": {
    "leagueRel": {
      "type": "belongsTo",
      "model": "league",
      "foreignKey": "leagueId"
    },
    "choices": {
      "type": "hasMany",
      "model": "choice",
      "foreignKey": "exactId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "founder",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
