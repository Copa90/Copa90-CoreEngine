{
  "name": "estimate",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "time": {
      "type": "number",
      "required": true
    },
    "checkTime": {
      "type": "number",
      "required": false,
      "default": 0
    },
    "status": {
      "type": "string",
      "required": false,
      "default": "Open"
    }
  },
  "validations": [],
  "relations": {
    "clientRel": {
      "type": "belongsTo",
      "model": "client",
      "foreignKey": "clientId"
    },
    "predictRel": {
      "type": "belongsTo",
      "model": "predict",
      "foreignKey": "predictId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "founder",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
