{
  "name": "transaction",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "time": {
      "type": "number",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "status": {
      "type": "string",
      "required": true
    },
    "receiptInfo": {
      "type": "object",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "clientRel": {
      "type": "belongsTo",
      "model": "client",
      "foreignKey": "clientId"
    },
    "packageRel": {
      "type": "belongsTo",
      "model": "package",
      "foreignKey": "packageId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "founder",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
