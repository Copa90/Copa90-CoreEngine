{
  "name": "challenge",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "creatorId": {
      "type": "number",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "reduceChances": {
      "type": "number",
      "required": true,
      "default": 10
    },
    "period": {
      "type": "number",
      "required": true,
      "default": 259200000
    }
  },
  "validations": [],
  "relations": {
    "clients": {
      "type": "hasMany",
      "model": "client",
      "foreignKey": "challengeId",
      "through": "competition"
    }
  },
  "acls": [],
  "methods": {}
}
