{
  "name": "client",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "realm",
    "emailVerified"
  ],
  "properties": {
    "password": {
      "type": "string",
      "required": true,
      "index": true,
      "min": 6
    },
    "email": {
      "type": "string",
      "required": true,
      "index": true
    },
    "username": {
      "type": "string",
      "required": true,
      "index": true
    },
    "time": {
      "type": "number",
      "required": true,
      "index": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "fullname": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "accountInfo": {
      "type": "embedsOne",
      "model": "accountInfo",
      "property": "accountInfoModel",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    },
    "transactions": {
      "type": "hasMany",
      "model": "transaction",
      "foreignKey": "clientId"
    },
    "estimates": {
      "type": "hasMany",
      "model": "estimate",
      "foreignKey": "clientId"
    },
    "champions": {
      "type": "hasMany",
      "model": "champion",
      "foreignKey": "clientId",
      "through": "ranking"
    },
    "challenges": {
      "type": "hasMany",
      "model": "challenge",
      "foreignKey": "clientId",
      "through": "competition"
    },
    "trophies": {
      "type": "hasMany",
      "model": "trophy",
      "foreignKey": "clientId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "changePassword"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "replaceById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__accountInfo"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__transactions"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__estimates"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__champions"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__findById__champions"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__challenges"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__findById__challenges"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "founder",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
