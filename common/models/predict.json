{
  "name": "predict",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "explanation": {
      "type": "string",
      "required": true
    },
    "weekNumber": {
      "type": "number",
      "required": true
    },
    "possibility": {
      "type": "number",
      "required": true
    },
    "point": {
      "type": "number",
      "required": true
    },
    "beginningTime": {
      "type": "number",
      "required": true
    },
    "endingTime": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "string",
      "required": false,
      "default": "Created"
    },
    "tag": {
      "type": "string",
      "required": false,
      "default": "Week"
    },
    "occurrence": {
      "type": "number",
      "required": false,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "leagueRel": {
      "type": "belongsTo",
      "model": "league",
      "foreignKey": "leagueId"
    },
    "estimates": {
      "type": "hasMany",
      "model": "estimate",
      "foreignKey": "predictId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "founder",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
